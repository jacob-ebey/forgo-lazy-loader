{
  "name": "foro-lazy-loader",
  "version": "1.0.0",
  "license": "MIT",
  "sideEffects": false,
  "homepage": "https://github.com/jacob-ebey/library-boilerplate",
  "author": {
    "name": "Jacob Ebey",
    "email": "jacob.ebey@live.com"
  },
  "files": [
    "lib"
  ],
  "types": "./lib/index.d.ts",
  "main": "./lib/index.js",
  "scripts": {
    "build": "node esbuild",
    "dev": "jest --watch",
    "format": "prettier --write ./src ./test",
    "test": "jest"
  },
  "devDependencies": {
    "@types/acorn": "4.0.5",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.31",
    "@types/supertest": "2.0.10",
    "@types/webpack": "4.41.26",
    "cz-conventional-changelog": "3.3.0",
    "esbuild": "0.8.53",
    "esbuild-jest": "0.5.0",
    "forgo": "0.0.73",
    "glob": "7.1.6",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "supertest": "6.1.3",
    "typescript": "4.2.2",
    "webpack": "5.24.3",
    "webpack-cli": "4.5.0"
  },
  "jest": {
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "<rootDir>/lib/"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx",
      "node",
      "cjs"
    ],
    "transform": {
      ".+\\.tsx?$": [
        "esbuild-jest",
        {
          "sourcemap": "external",
          "jsxFactory": "h",
          "jsxFragment": "Fragment"
        }
      ]
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "acorn": "8.0.5",
    "acorn-walk": "8.0.2",
    "astring": "1.7.0",
    "webpack-stats-plugin": "1.0.3"
  }
}
